<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.mycompany.miniproject.dto.NoticeDTO" alias="Notice" />
		<typeAlias type="com.mycompany.miniproject.dto.HelpdeskDTO" alias="Helpdesk" />
		<typeAlias type="com.mycompany.miniproject.dto.CommentDTO" alias="Comment" />
		<typeAlias type="com.mycompany.miniproject.dto.CartDTO" alias="Cart"/>
		<typeAlias type="com.mycompany.miniproject.dto.OrderDTO" alias="Order"/>	
		<typeAlias type="com.mycompany.miniproject.dto.ProductReviewDTO" alias="ProductReview"/>
		<typeAlias type="com.mycompany.miniproject.dto.CenterImageDTO" alias="CenterImage"/>
		<typeAlias type="com.mycompany.miniproject.dto.ProductDTO" alias="Product"/>
		<typeAlias type="com.mycompany.miniproject.dto.MemberDTO" alias="Member"/>
		<typeAlias type="com.mycompany.miniproject.dto.CategoryDTO" alias="Category"/>
		<typeAlias type="com.mycompany.miniproject.dto.Pager" alias="Pager"/>
	</typeAliases>
	<typeHandlers>
		<typeHandler handler="com.mycompany.miniproject.type.CategoryTypeHandler" javaType="com.mycompany.miniproject.type.Category"/>
		<typeHandler handler="com.mycompany.miniproject.type.ProductStateTypeHandler" javaType="com.mycompany.miniproject.type.ProductState"/>
		<typeHandler handler="com.mycompany.miniproject.type.ProductUsecaseTypeHandler" javaType="com.mycompany.miniproject.type.ProductUsecase"/>
	</typeHandlers>
</configuration>
